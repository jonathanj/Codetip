@javascript 'scripts', ->
    @bundles 'codetip'
    @options
        build: './static/js'
        compress: true

    @coffeescript './coffee'
    @js './vendor'


@stylesheets 'styles', ->
    @bundles 'codetip'
    @options
        build: './static/css'
        compress: true

    @stylus './stylus'
    @css './vendor'
